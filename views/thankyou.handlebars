    <script>

      $(document).ready(function () {
        var storage = window.sessionStorage;


        if (typeof (Storage) !== "undefined") {

          var newReservation = JSON.parse(storage.getItem('clientReservation'));
          var newClientReservationTotalCost = JSON.parse(storage.getItem('clientReservationTotalCost'));

          var numOfNights = newReservation.NumberofNights;
          var checkIn = newReservation.BeginningDate;
          var checkOut = newReservation.EndingDate;
          var numAdults = newReservation.Adults;
          var totalCost = newClientReservationTotalCost.totalCost;

          document.getElementById("checkIn").innerHTML = "Check In: " + checkIn;
          document.getElementById("checkOut").innerHTML = "Check Out: " + checkOut;
          document.getElementById("numOfAdults").innerHTML = "Number of Adults: " + numAdults;
          document.getElementById("totalNights").innerHTML = "Number of Nights: " + numOfNights;
          document.getElementById("totalCost").innerHTML = "Total Cost for Stay: " + totalCost;


        } else {
          document.getElementById("result").innerHTML = "Sorry, your browser does not support Web Storage...";
        }

        $('.parallax').parallax();
      });

      $(document).ready(function () {
        $('.carousel').carousel();
      });
      $(document).ready(function () {
        $('.scrollspy').scrollSpy();
